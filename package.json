{
  "name": "ficat-vue",
  "version": "1.0.0",
  "description": "Nuxt-Vue.js Ficat version",
  "author": "Lakshamana",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "debug": "cross-env NODE_ENV=development DEBUG=koa* nodemon --harmony server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "seed:make": "knex seed:make",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:test": "cross-env NODE_ENV=test knex seed:run",
    "seed:travis": "cross-env NODE_ENV=travis knex seed:run",
    "migrate": "knex migrate:latest",
    "migrate:test": "cross-env NODE_ENV=test knex migrate:latest",
    "migrate:travis": "cross-env NODE_ENV=travis knex migrate:latest",
    "makedb": "npm run migrate && npm run seed",
    "makedb:test": "npm run migrate:test && npm run seed:test",
    "makedb:travis": "npm run migrate:travis && npm run seed:travis",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test": "jest --maxWorkers=8 --force-exit --detectOpenHandles",
    "test:travis": "cross-env NODE_ENV=travis jest --maxWorkers=8 --force-exit --detectOpenHandles",
    "ci": "npm run lint",
    "knex": "knex"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@koa/cors": "~3.0.0",
    "@nuxtjs/axios": "~5.3.6",
    "@nuxtjs/recaptcha": "~0.6.2",
    "@vue/babel-helper-vue-jsx-merge-props": "~1.0.0",
    "@vue/babel-preset-jsx": "~1.1.2",
    "axios": "~0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "bcrypt": "~5.0.0",
    "bookshelf": "~0.15.1",
    "bookshelf-secure-password": "~4.0.0",
    "chart.js": "~2.8.0",
    "cross-env": "~5.2.0",
    "dotenv": "~8.0.0",
    "html-pdf": "~2.2.0",
    "jsonwebtoken": "~8.5.1",
    "knex": "~0.19.3",
    "koa": "~2.6.2",
    "koa-body": "~4.1.1",
    "koa-router": "~7.4.0",
    "koa-static": "~5.0.0",
    "koa-unless": "~1.0.7",
    "mysql": "~2.18.1",
    "nodemailer": "~6.4.2",
    "nuxt": "^2.14.7",
    "nuxt-buefy": "~0.3.13",
    "nuxt-fontawesome": "~0.4.0",
    "p-debounce": "~2.1.0",
    "vue-cookies": "~1.5.13",
    "vuelidate": "~0.7.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "~1.0.1",
    "@nuxtjs/eslint-module": "~1.0.0",
    "@vue/test-utils": "~1.0.0-beta.27",
    "babel-eslint": "~10.0.1",
    "babel-jest": "^26.6.1",
    "babel-plugin-dynamic-import-webpack": "~1.1.0",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "eslint": "~6.1.0",
    "eslint-config-prettier": "~4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "~3.0.1",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "prettier": "~1.16.4",
    "vue-jest": "^3.0.7"
  }
}
